{
    "name": "edriving-limited/custom-sqs-driver",
    "description": "Adds support for custom SQS payloads in Laravel",
    "keywords": [
        "edriving-limited",
        "laravel",
        "custom-sqs-driver"
    ],
    "homepage": "https://github.com/edriving-limited/custom-sqs-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Keiron Lowe",
            "email": "keiron.lowe@edriving.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1|^8.2",
        "aws/aws-sdk-php": "^3.269",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "eDriving\\CustomSqsDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eDriving\\CustomSqsDriver\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "eDriving\\CustomSqsDriver\\Providers\\CustomSqsDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
